{
	"name": "ritzkick",
	"private": true,
	"scripts": {
		"start": "env-cmd -f ./env/.prod.env node --no-deprecation server.js",
		"start-test": "env-cmd -f ./env/.prod.test.env node server.js",
		"dev": "env-cmd -f ./env/.local.env nodemon --no-deprecation server.js",
		"build": "next build",
		"analyze": "cross-env ANALYZE=true next build",
		"postbuild": "next-sitemap",
		"lint": "next lint",
		"test": "env-cmd -f ./env/.test.env jest --runInBand",
		"test-ci": "env-cmd -f ./env/.test.ci.env jest --runInBand ",
		"test-dev": "env-cmd -f ./env/.test.env jest --watch --runInBand",
		"format": "prettier --config .prettierrc './**/*.js' './**/*.css' --write"
	},
	"jest": {
		"testEnvironment": "node",
		"testTimeout": 10000
	},
	"dependencies": {
		"@emotion/react": "^11.7.1",
		"@emotion/styled": "^11.6.0",
		"@fortawesome/fontawesome-svg-core": "^1.2.36",
		"@fortawesome/free-brands-svg-icons": "^5.15.4",
		"@fortawesome/free-regular-svg-icons": "^5.15.4",
		"@fortawesome/free-solid-svg-icons": "^5.15.4",
		"@fortawesome/react-fontawesome": "^0.1.17",
		"@mui/icons-material": "^5.4.2",
		"@mui/material": "^5.4.2",
		"@mui/styled-engine-sc": "^5.4.2",
		"@next/bundle-analyzer": "^12.0.10",
		"@sendgrid/mail": "^7.6.1",
		"axios": "^0.25.0",
		"bcryptjs": "^2.4.3",
		"chalk": "^4.1.2",
		"chart.js": "^3.7.0",
		"chartjs-adapter-moment": "^1.0.0",
		"chartjs-plugin-zoom": "^1.2.0",
		"compression": "^1.7.4",
		"cross-env": "^7.0.3",
		"cryptocurrencies": "^7.0.0",
		"env-cmd": "^10.1.0",
		"express": "^4.17.2",
		"express-jsdoc-swagger": "^1.6.7",
		"helmet": "^5.0.2",
		"jsonwebtoken": "^8.5.1",
		"moment": "^2.29.1",
		"mongoose": "^6.1.8",
		"next": "^12.1.0",
		"next-i18next": "^10.3.0",
		"next-sitemap": "^2.1.9",
		"nodemailer": "^6.7.2",
		"react": "17.0.2",
		"react-bootstrap": "^2.1.1",
		"react-chartjs-2": "^4.0.1",
		"react-dom": "17.0.2",
		"react-google-login": "^5.2.2",
		"react-hook-form": "^7.27.0",
		"react-hooks-use-modal": "^2.1.0",
		"react-select": "^5.2.2",
		"sass": "^1.49.0",
		"sendgrid": "^5.2.3",
		"socket.io": "^4.4.1",
		"socket.io-client": "^4.4.1",
		"spdy": "^4.0.2",
		"styled-components": "^5.3.3",
		"validator": "^13.7.0"
	},
	"devDependencies": {
		"eslint": "8.7.0",
		"eslint-config-next": "12.0.9",
		"eslint-config-prettier": "^8.3.0",
		"eslint-plugin-prettier": "^4.0.0",
		"jest": "^27.4.7",
		"nodemon": "^2.0.15",
		"prettier": "2.5.1",
		"supertest": "^6.2.2"
	},
	"resolve": {
		"alias": {
			"chart.js": "${path.resolve(SRC_PATH, 'empty')}"
		}
	}
}
