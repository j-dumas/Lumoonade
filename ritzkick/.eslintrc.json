{
	"plugins": ["prettier"],
	"extends": ["eslint:recommended", "next/core-web-vitals", "prettier"],
	"rules": {
		"no-console": 1, // Means warning
		"prettier/prettier": 2, // Means error
		"react-hooks/rules-of-hooks": "off",
		"sort-imports": "error",
		"no-duplicate-imports": "error",
		"spaced-comment": "error"
	}
}
